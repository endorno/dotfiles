[user]
	name = Teppei Fujisawa
	email = teppei.fujisawa@gmail.com
[alias]
	ci = commit
	co = checkout
	st = status
  br = branch
  tr = log --graph --pretty='format:%C(yellow)%h%Creset %s %Cgreen(%an)%Creset %Cred%d%Creset'
  hist = for-each-ref --sort=-committerdate 'refs/heads/'
[push]
	default = matching
[core]
  excludesfile = /Users/teppei.fujisawa/.gitignore
	editor = vim
[git-media]
  transport = local
  localpath = /tmp/git-media

[color]
	ui = true
[credential]
	helper = osxkeychain
[hub]
	host = github.dena.jp
